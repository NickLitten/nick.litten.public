{
  "files.trimTrailingWhitespace": true,
  "indent": 3,
  "BlankStructNamesCheck": false,
  "QualifiedCheck": true,
  "PrototypeCheck": true,
  "ForceOptionalParens": true,
  "NoOCCURS": true,
  "NoSELECTAll": true,
  "UselessOperationCheck": true,
  "UppercaseConstants": true,
  "IncorrectVariableCase": true,
  "RequiresParameter": true,
  "RequiresProcedureDescription": true,
  "StringLiteralDupe": false,
  "RequireBlankSpecial": false,
  "CopybookDirective": "include",
  "NoSQLJoins": true,
  "NoGlobalsInProcedures": true,
  "SpecificCasing": [
    {"operation": "Dcl-Proc", "expected": "Dcl-Proc"},
    {"operation": "Dcl-Pi", "expected": "Dcl-Pi"},
    {"operation": "Dcl-C", "expected": "Dcl-C"},
    {"operation": "Dcl-Ds", "expected": "Dcl-Ds"},
    {"operation": "Dcl-S", "expected": "Dcl-S"},
    {"operation": "%Parms", "expected": "%Parms"},
    {"operation": "Const", "expected": "Const"},
    {"operation": "Options", "expected": "Options"},
    {"operation": "Value", "expected": "Value"},
    {"operation": "Select", "expected": "Select"},
    {"operation": "When", "expected": "When"},
    {"operation": "Other", "expected": "Other"},
    {"operation": "EndSl", "expected": "EndSl"},
    {"operation": "Return", "expected": "Return"},
    {"operation": "If", "expected": "If"},
    {"operation": "Else", "expected": "Else"},
    {"operation": "EndIf", "expected": "EndIf"}
   ],
  "NoCTDATA": false,
  "PrettyComments": true,
  "NoGlobalSubroutines": true,
  "NoLocalSubroutines": false,
  "NoUnreferenced": true,
  "NoExternalTo": [
    "QCMD",
    "QP2TERM",
    "QSH",
    "SYSTEM",
    "QCMDEXC"
  ],
  "NoExecuteImmediate": false,
  "NoExtProgramVariable": true,
  "IncludeMustBeRelative": true,
  "SQLHostVarCheck": true,
  "RequireOtherBlock": false
}