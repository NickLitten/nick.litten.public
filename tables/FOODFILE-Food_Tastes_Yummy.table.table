CREATE or replace TABLE
    FOODFILE (
        INGID DECIMAL(10, 0) NOT NULL PRIMARY KEY,
        INGNAME VARCHAR(30) NOT NULL,
        CATEGORY VARCHAR(20),
        MEASURE VARCHAR(10),
        QUANTITY DECIMAL(7, 2),
        EXPDATE DATE,
        ORGANIC CHAR(1) CHECK (ORGANIC IN ('Y', 'N'))
    );