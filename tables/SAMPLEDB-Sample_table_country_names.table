-- ============================================================================
-- Summary Statistics View
-- ============================================================================
CREATE OR REPLACE VIEW
    V_SAMPLEDB_SUMMARY AS
SELECT
    REGION,
    COUNT(*) AS COUNTRY_COUNT,
    SUM(
        CASE
            WHEN EU_MEMBER = 'Y' THEN 1
            ELSE 0
        END
    ) AS EU_MEMBERS,
    SUM(POPULATION) AS TOTAL_POPULATION,
    SUM(AREA_KM2) AS TOTAL_AREA_KM2,
    AVG(POPULATION) AS AVG_POPULATION
FROM
    SAMPLEDB
GROUP BY
    REGION
ORDER BY
    REGION;


COMMENT ON VIEW V_SAMPLEDB_SUMMARY IS 'Summary statistics of European countries by region';