-- ============================================================================
-- Table: SAMPLEDB
-- Description: Sample table containing European country information
-- Author: Nick Litten
-- Created: 2026-02-06
-- ============================================================================
DROP INDEX IDX_SAMPLEDB_NAME;
DROP INDEX IDX_SAMPLEDB_REGION;
DROP INDEX IDX_SAMPLEDB_EU;
DROP VIEW V_SAMPLEDB_SUMMARY;

CREATE OR REPLACE TABLE
    SAMPLEDB (
        -- Primary Key: ISO 3166-1 alpha-2 country code
        COUNTRY_CODE CHAR(2) NOT NULL,
        -- Country information
        COUNTRY_NAME VARCHAR(100) NOT NULL,
        COUNTRY_NAME_LOCAL VARCHAR(100),
        CAPITAL_CITY VARCHAR(100),
        -- Geographic and demographic data
        REGION VARCHAR(50) NOT NULL,
        POPULATION DECIMAL(12, 0),
        AREA_KM2 DECIMAL(12, 2),
        -- EU membership
        EU_MEMBER CHAR(1) DEFAULT 'N' NOT NULL,
        EU_JOIN_DATE DATE,
        -- Currency information
        CURRENCY_CODE CHAR(3),
        CURRENCY_NAME VARCHAR(50),
        -- Audit fields
        CREATED_AT TIMESTAMP DEFAULT CURRENT_TIMESTAMP NOT NULL,
        UPDATED_AT TIMESTAMP DEFAULT CURRENT_TIMESTAMP NOT NULL,
        -- Primary key constraint
        CONSTRAINT PK_SAMPLEDB PRIMARY KEY (COUNTRY_CODE),
        -- Check constraints
        CONSTRAINT CHK_SAMPLEDB_EU_MEMBER CHECK (EU_MEMBER IN ('Y', 'N')),
        CONSTRAINT CHK_SAMPLEDB_POPULATION CHECK (
            POPULATION IS NULL
            OR POPULATION >= 0
        ),
        CONSTRAINT CHK_SAMPLEDB_AREA CHECK (
            AREA_KM2 IS NULL
            OR AREA_KM2 > 0
        ),
        CONSTRAINT CHK_SAMPLEDB_NAME CHECK (LENGTH(TRIM(COUNTRY_NAME)) > 0)
    ) RCDFMT SAMPLEDBR;


-- ============================================================================
-- Indexes for performance optimization
-- ============================================================================
CREATE INDEX IDX_SAMPLEDB_NAME ON SAMPLEDB (COUNTRY_NAME);

CREATE INDEX IDX_SAMPLEDB_REGION ON SAMPLEDB (REGION);

CREATE INDEX IDX_SAMPLEDB_EU ON SAMPLEDB (EU_MEMBER);


-- ============================================================================
-- Comments for documentation
-- ============================================================================
COMMENT ON TABLE SAMPLEDB IS 'European countries reference table with geographic and demographic data';


COMMENT ON COLUMN SAMPLEDB.COUNTRY_CODE IS 'ISO 3166-1 alpha-2 country code (Primary Key)';


COMMENT ON COLUMN SAMPLEDB.COUNTRY_NAME IS 'Official country name in English';


COMMENT ON COLUMN SAMPLEDB.COUNTRY_NAME_LOCAL IS 'Country name in local language';


COMMENT ON COLUMN SAMPLEDB.CAPITAL_CITY IS 'Capital city name';


COMMENT ON COLUMN SAMPLEDB.REGION IS 'European region (e.g., Western Europe, Eastern Europe, Nordic)';


COMMENT ON COLUMN SAMPLEDB.EU_MEMBER IS 'European Union membership flag: Y=Yes, N=No';


